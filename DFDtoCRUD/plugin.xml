					style="normal"
          tooltip="Chart Diagram"
          icon="icons/chart_icon.png"
					toolbarPath="com.vp.plugin.sample.chartdiagram.actionset1.toolbar1/#">
				<actionController class="com.vp.plugin.sample.chartdiagram.actions.ChartDiagramActionControl"/>
			</action>
			
		</actionSet>
	</actionSets>	
