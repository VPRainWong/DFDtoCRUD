		class="com.vp.plugin.sample.chartdiagram.ChartDiagramPlugin">
	
	<actionSets>
		<actionSet id="com.vp.plugin.sample.chartdiagram.actionset1">
			<toolbar
					id="com.vp.plugin.sample.chartdiagram.actionset1.toolbar1"
					orientation="north"
					index="last"/>
			
			<action
					id="com.vp.plugin.sample.chartdiagram.actionset1.ChartDiagramAction"
					actionType="generalAction"
      		name="Chart Diagram"
